{
	"id": "tools/open-cmsis-pack/ctools",
	"version": "1.6.0-dev0",
	"description": "The open-source Open-CMSIS-Pack tools make it easy to create and validate CMSIS-Packs as well as compose and build Embedded Arm Cortex-M Microcontroller projects using CMSIS-Packs.",
	"summary": "Open-CMSIS-Pack cli tools",
	"contacts": {
		"Joachim Krech": {
			"email": "joachim.krech@linaro.org",
			"role": "publisher"
		},
		"ctools": {
			"email": "open-cmsis-pack-dev@op-lists.linaro.org",
			"role": "originator"
		}
	},
	"demands": {
		"windows and x64": {
			"install": {
				"unzip": "https://github.com/Open-CMSIS-Pack/cmsis-toolbox/releases/download/1.6.0-dev0/cmsis-toolbox-windows-amd64.zip",
				"sha256": "",
				"strip": 1
			},
			"exports": {
				"paths": {
					"PATH": "bin"
				}
			}
		},
		"linux and x64": {
			"install": {
				"untar": "https://github.com/Open-CMSIS-Pack/cmsis-toolbox/releases/download/1.6.0-dev0/cmsis-toolbox-linux-amd64.tar.gz",
				"sha256": "",
				"strip": 1
			}
		},
		"linux and arm64": {
			"install": {
				"untar": "https://github.com/Open-CMSIS-Pack/cmsis-toolbox/releases/download/1.6.0-dev0/cmsis-toolbox-linux-arm64.tar.gz",
				"sha256": "",
				"strip": 1
			}
		},
		"osx": {
			"install": {
				"untar": "https://github.com/Open-CMSIS-Pack/cmsis-toolbox/releases/download/1.6.0-dev0/cmsis-toolbox-darwin-amd64.tar.gz",
				"sha256": "",
				"strip": 1
			}
		},
		"not windows": {
			"exports": {
				"paths": {
					"PATH": "bin"
				}
			}
		}
	}
}
